{
    "name": "berdidajohnlouise/file-upload",
    "description": "A package for uploading files in php",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "8.5.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Berdidajohnlouise\\FileUpload\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Berdidajohnlouise\\FileUpload\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "John Louise Berdida"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "7.x-dev",
        "symfony/http-foundation": "^5.0",
        "illuminate/filesystem": "^7.0"
    }
}
